{
	"name": "webpack-template",
	"version": "0.0.1",
	"description": "webpack-template",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack serve --config ./config/dev.cfg.js",
		"build": "cross-env NODE_ENV=production webpack --progress --config ./config/prod.cfg.js && size-limit",
		"lint": "eslint src/*.js",
		"lint-fix": "eslint --fix src/*.js",
		"server": "node ./config/server/server.js",
		"sortmq": "node ./config/helpers/sortmq.js",
		"purify": "node ./config/helpers/purify.js",
		"size": "size-limit"
	},
	"size-limit": [
		{
			"path": "./dist/js/*.js",
			"limit": "2000 ms"
		}
	],
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.13.8",
		"@babel/eslint-parser": "^7.13.8",
		"@babel/preset-env": "^7.13.9",
		"@babel/preset-react": "^7.12.13",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
		"@size-limit/preset-app": "^4.10.1",
		"autoprefixer": "^10.2.5",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^8.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.1.1",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"css-mqpacker": "^7.0.0",
		"eslint": "^7.21.0",
		"eslint-webpack-plugin": "^2.5.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^5.3.0",
		"image-webpack-loader": "^7.0.1",
		"mini-css-extract-plugin": "^1.3.9",
		"node-sass": "^5.0.0",
		"node-static": "^0.7.11",
		"postcss": "^8.2.7",
		"postcss-loader": "^5.1.0",
		"purify-css": "^1.2.5",
		"react-refresh": "^0.9.0",
		"sass-loader": "^11.0.1",
		"size-limit": "^4.10.1",
		"stylelint": "^13.12.0",
		"svgo": "^2.2.1",
		"svgo-loader": "^3.0.0",
		"webpack": "^5.24.4",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.7.3"
	},
	"dependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	}
}
